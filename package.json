{
  "name": "@okee-uikit/m-vue",
  "version": "0.1.5",
  "author": "Totoo",
  "license": "MIT",
  "description": "Mobile UI Components built on Vue for Oceanengine",
  "main": "lib/index.js",
  "style": "lib/index.css",
  "module": "es/index.js",
  "typings": "lib/index.d.ts",
  "files": [
    "build",
    "es",
    "lib",
    "svg",
    "types",
    "postcss.config.js"
  ],
  "sideEffects": [
    "es/**/style/*",
    "lib/**/style/*",
    "*.css",
    "*.less"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:oceanengine/okeedesign-mobile-vue.git"
  },
  "keywords": [
    "vue",
    "component",
    "mobile",
    "oceanengine"
  ],
  "scripts": {
    "dev": "npm run build:entry && webpack-dev-server --config ./build/webpack.site.dev.js",
    "dev:theme": "cross-env THEME=true npm run dev",
    "dev:aweme": "cross-env THEME=aweme npm run dev",
    "lint": "npm run lint:script && npm run lint:style",
    "lint:script": "eslint . --ext .js,.ts,.tsx,.vue,.js --quiet",
    "lint:style": "stylelint \"src/**/*.less\"",
    "fix:style": "stylelint \"src/**/*.less\" --fix",
    "build": "npm run ./build:compile && npm run build:dist",
    "build:compile": "cross-env NODE_ENV=production node ./build/run.js compile",
    "build:dist": "node ./build/run.js dist",
    "build:entry": "node ./build/run.js build-entry",
    "build-script": "node ./build/build-lib.js",
    "build-script:entry": "node ./build/build-entry.js",
    "ct": "git-cz",
    "prettier-format": "prettier --write .",
    "deploy": "cd ./docs && sh docs_build.sh",
    "test": "jest",
    "test:cov": "jest && npm run coverall",
    "coverall": "cat coverage/lcov.info | coveralls"
  },
  "dependencies": {
    "@babel/runtime": "^7.9.2",
    "@vue/babel-helper-vue-jsx-merge-props": "^1.0.0",
    "colors": "^1.4.0",
    "csso": "^4.0.2",
    "fast-glob": "^3.1.0",
    "fs-extra": "^8.1.0",
    "html-loader": "^0.5.5",
    "less": "^3.10.3",
    "normalize.css": "^8.0.1",
    "postcss": "^7.0.21",
    "postcss-load-config": "^2.1.0",
    "postcss-px2rem": "^0.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-syntax-jsx": "^7.8.3",
    "@babel/plugin-transform-object-assign": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-typescript": "^7.9.0",
    "@commitlint/cli": "^7.5.2",
    "@commitlint/config-conventional": "^7.5.0",
    "@icon-park/vue": "^1.3.3",
    "@lerna/project": "^3.13.1",
    "@mdx-js/vue-loader": "^1.6.22",
    "@types/webpack-env": "^1.14.1",
    "@typescript-eslint/eslint-plugin": "^4.8.1",
    "@typescript-eslint/parser": "^4.8.1",
    "@vue/babel-preset-jsx": "^1.1.2",
    "@vue/cli-plugin-unit-jest": "~4.5.11",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "@vue/test-utils": "1.1.2",
    "autoprefixer": "^9.8.6",
    "babel-loader": "^8.1.0",
    "colorful": "^2.1.0",
    "commander": "^6.2.0",
    "commitizen": "^3.0.7",
    "coveralls": "^3.1.1",
    "cross-env": "^5.2.0",
    "css-loader": "^3.0.0",
    "cz-lerna-changelog": "^2.0.2",
    "d3": "^6.2.0",
    "del": "^5.1.0",
    "dependency-tree": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-vue": "^7.1.0",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-strip-code": "^0.1.4",
    "gulp-typescript": "^6.0.0-alpha.1",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.0.0",
    "is-windows": "^1.0.2",
    "jsdom-global": "^3.0.2",
    "less-loader": "^5.0.0",
    "less-plugin-npm-import": "^2.1.0",
    "lodash": "^4.17.20",
    "merge2": "^1.4.1",
    "minimist": "^1.2.5",
    "postcss-loader": "^3.0.0",
    "prettier": "^2.1.2",
    "rimraf": "^3.0.2",
    "shelljs": "^0.8.3",
    "signale": "^1.4.0",
    "style-loader": "^0.23.1",
    "stylelint": "^13.8.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^18.3.0",
    "stylelint-order": "^3.0.0",
    "through2": "^4.0.2",
    "typescript": "^3.8.3",
    "uppercamelcase": "^3.0.0",
    "url-loader": "^2.0.1",
    "vue": "^2.6.10",
    "vue-loader": "^15.7.0",
    "vue-router": "^3.0.7",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.35.3",
    "webpack-cli": "^3.3.5",
    "webpack-dev-server": "^3.7.2",
    "webpack-merge": "^4.2.2"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-lerna-changelog"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e $GIT_PARAMS",
      "pre-commit": "npm run lint"
    }
  },
  "browserslist": [
    "Android >= 4.0",
    "iOS >= 7"
  ]
}
